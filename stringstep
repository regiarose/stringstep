49', 'D54', 'D8', 'D48', 'A45', 'C51', 'D53', 'A48', 'C36', 'D56', 'A29', 'C52', 'A38', 'C26', 'D32', 'B3', 'B56', 'D42', 'A22', 'B31', 'D44', 'C25', 'B39', 'C29', 'A21', 'B2', 'D46', 'A58', 'C50', 'C15', 'D9', 'A32', 'C58', 'C6', 'B54', 'B7', 'A25', 'C28', 'B30', 'D14', 'A11', 'D33', 'A34', 'C58', 'B27', 'A35', 'C34', 'D45', 'A30', 'B23', 'C39', 'A17', 'A51', 'B56', 'C38', 'C1', 'D11', 'C24', 'C55', 'C9', 'D30', 'B37', 'A19', 'C41', 'D28', 'B42', 'D36', 'B18', 'D30', 'C15', 'D36', 'A53', 'D50', 'C54', 'B35', 'A12', 'B11', 'A24', 'D59', 'C41', 'A52', 'B55', 'B6', 'D57', 'A5', 'A43', 'D58', 'B2', 'D5', 'A39', 'C42', 'B22'];
        let index = -1;
        let autoplay = false;
        let intervalId = null;
        let speed = 2000;

        function showStep() {
            if (index >= 0 && index < steps.length) {
                document.getElementById('step').textContent = steps[index];
                document.getElementById('counter').textContent = (index + 1) + " / " + steps.length;
            } else if (index >= steps.length) {
                document.getElementById('step').textContent = "KÉSZ!";
                document.getElementById('counter').textContent = "";
                stopAutoplay();
            } else {
                document.getElementById('step').textContent = "Kattints a „Következő” gombra";
                document.getElementById('counter').textContent = "";
            }
        }

        function nextStep() {
            if (index < steps.length) {
                index++;
                showStep();
            }
        }

        function prevStep() {
            if (index > 0) {
                index--;
                showStep();
            }
        }

        function jumpToStep() {
            const input = document.getElementById("jumpInput").value;
            const stepNum = parseInt(input);
            if (!isNaN(stepNum) && stepNum >= 1 && stepNum <= steps.length) {
                index = stepNum - 1;
                showStep();
            }
        }

        function toggleAutoplay() {
            autoplay = document.getElementById("autoplayToggle").checked;
            if (autoplay) {
                intervalId = setInterval(nextStep, speed);
            } else {
                stopAutoplay();
            }
        }

        function stopAutoplay() {
            clearInterval(intervalId);
            document.getElementById("autoplayToggle").checked = false;
            autoplay = false;
        }

        function updateSpeed() {
            let val = document.getElementById("speedSlider").value;
            speed = val * 1000;
            document.getElementById("speedValue").textContent = val;
            if (autoplay) {
                stopAutoplay();
                intervalId = setInterval(nextStep, speed);
                autoplay = true;
                document.getElementById("autoplayToggle").checked = true;
            }
        }
    </script>
</body>
</html>
